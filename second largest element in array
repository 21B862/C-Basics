#include <iostream>
using namespace std;
int main()

{   
    int arr[5],i;
    int largest=arr[0];
    
cout<<"enter the elements";
  
  for(i=0;i<5;i++)
  {
      cin>>arr[i];
  }
  
  for(i=1;i<5;i++)
  {
      if(arr[i]>largest)
      {largest=arr[i];}
  }
  
  int s_largest=0;
  for(i=0;i<5;i++)
  {
      if(arr[i]>s_largest&&arr[i]!=largest)
      s_largest=arr[i];
  }
  
  cout<<"the second largest element is"<<s_largest;
   
    return 0;

}
