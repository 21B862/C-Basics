#include<iostream>
using namespace std;
int main()
{
    int arr[5],i,sum=0;
    cout<<"Enter the array elements\n";
    for(i=0;i<5;i++)
    {
        cin >> arr[i];
    }
    cout<<"The even elements are \n";
    for(i=0;i<5;i++)
    {
        if(arr[i]%2==0)
        {
        cout<<"\t"<<arr[i];
        }
    }
    
    return 0;
}
